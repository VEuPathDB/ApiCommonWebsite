      parameter="org.gusdb.wdk.controller.action.standard.LoggingAction"
      type="org.gusdb.wdk.controller.actionutil.WdkActionWrapper"
      scope="request"
      validate="false">
    </action>
    <action path="/wizard"
      type="org.gusdb.wdk.controller.action.WizardAction"
      scope="request"
      name="wizardForm"
      validate="false">
      <forward name="show_wizards" path="/wdk/jsp/wizard/wizards.jsp"/>
    </action>
    <action
      path="/processBoolean"
      type="org.gusdb.wdk.controller.action.ProcessBooleanAction"
      scope="request"
      validate="true"
      name="questionForm">
      <forward name="show_strategy" path="/showStrategy.do"/>
    </action>
    <action
      path="/processStep"
      type="org.gusdb.wdk.controller.action.ProcessStepAction"
      scope="request"
      validate="false"
      name="questionForm">
      <forward name="show_strategy" path="/showStrategy.do"/>
    </action>
    <action
      path="/getDataset"
      type="org.apidb.apicommon.controller.action.GetDatasetAction"
      scope="request">
      <forward name="show_list" redirect="false" path="/wdkCustomization/jsp/DatasetList.jsp"/>
      <forward name="show_detail" redirect="false" path="/wdkCustomization/jsp/DatasetDetail.jsp"/>
      <forward name="show_xml_list" redirect="false" path="/showXmlDataList.do?name=XmlQuestions.Datasets"/>
      <forward name="show_xml_detail" redirect="false" path="/showXmlDataContent.do?name=XmlQuestions.Datasets"/>
    </action>
    <action
      path="/exportBasket"
      type="org.apidb.apicommon.controller.action.ExportBasketAction"
      scope="request"
      validate="false">
    </action>
    <action
      path="/showSummaryView"
      type="org.gusdb.wdk.controller.action.ShowSummaryViewAction"
      scope="request"
      validate="true">
    </action>
    <action
      path="/showRecordView"
      type="org.gusdb.wdk.controller.action.ShowRecordViewAction"
      scope="request"
      validate="true">
    </action>
    <action
      path="/invokeAttributePlugin"
      type="org.gusdb.wdk.controller.action.InvokeAttributePluginAction"
      scope="request"
      validate="false">
      <forward name="display" redirect="false" path="/wdk/jsp/results/attributePlugin.jsp"/>
    </action>
    <action
      path="/exportCytoscapeNetwork"
      type="org.apidb.apicommon.controller.action.ExportCytoscapeNetworkAction"
      scope="request"
      validate="false">
    </action>

    <action
      path="/siteSearch"
      type="org.apidb.apicommon.controller.action.SiteSearchAction"
      validate="false">
      <forward name="to-question" redirect="false" path="/processQuestion.do?questionSubmit=Get+Answer"/>
      <forward name="to-summary" redirect="false" path="/wdkCustomization/jsp/SiteSearchSummary.jsp"/>
      <forward name="to-html" redirect="false" path="/wdkCustomization/jsp/HtmlSearchSummary.jsp" />
    </action>


  </action-mappings>

  <controller maxFileSize="10M" contentType="text/html; charset=ISO-8859-1" />

  <message-resources parameter="org.gusdb.wdk.view.DefaultViewResources" null="false"/>

</struts-config>
