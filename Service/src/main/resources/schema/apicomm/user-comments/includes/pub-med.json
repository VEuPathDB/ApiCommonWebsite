{
  "$schema": "http://json-schema.org/draft-04/schema",
  "type": "object",
  "properties": {
    "id": {
      "description": "PubMed resource id",
      "type": "string",
      "maxLength": 100
    },
    "title": {
      "description": "Title of PubMed publication",
      "type": ["string", "null"]
    },
    "journal": {
      "description": "Publication journal",
      "type": ["string", "null"]
    },
    "author": {
      "description": "Publication author(s)",
      "type": ["string", "null"]
    },
    "url": {
      "description": "URL of PubMed resource",
      "type": ["string", "null"]
    },
    "status": {
      "type": "string"
    }
  },
  "additionalProperties": false,
  "required": [
    "id",
    "title",
    "author",
    "url"
  ]
}
